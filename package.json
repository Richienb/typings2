{
    "name": "typings2",
    "description": "A familiar interface to install Typescript typings, adapted from the now deprecated typings package.",
    "keywords": [
        "typescript",
        "typings",
        "node"
    ],
    "version": "0.0.0",
    "main": "dist/index.js",
    "files": [
        "src/**/*",
        "dist/**/*"
    ],
    "preferGlobal": true,
    "bin": {
        "typings": "dist/index.js"
    },
    "repository": "https://github.com/Richienb/typings2.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "yarn js --watch",
        "build": "yarn js",
        "js": "microbundle --target node --format cjs --no-sourcemap --external assert,stream,constants,util,bluebird,chalk,dot-prop,execa,find-cache-dir,fs-extra,js-yaml,libnpmsearch,lodash,npm-registry-fetch,yargs",
        "lint": "eslint src --ext .ts"
    },
    "dependencies": {
        "bluebird": "^3.5.5",
        "chalk": "^2.4.2",
        "dot-prop": "^5.1.0",
        "execa": "^3.0.0",
        "find-cache-dir": "^3.0.0",
        "fs-extra": "^8.1.0",
        "js-yaml": "^3.13.1",
        "libnpmsearch": "^2.0.2",
        "lodash": "^4.17.15",
        "npm-registry-fetch": "^4.0.1",
        "yargs": "^14.0.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.27",
        "@types/fs-extra": "^8.0.0",
        "@types/js-yaml": "^3.12.1",
        "@types/libnpmsearch": "^2.0.1",
        "@types/lodash": "^4.14.141",
        "@types/which-pm": "^1.0.0",
        "@types/yargs": "^13.0.3",
        "@typescript-eslint/eslint-plugin": "^2.3.2",
        "@typescript-eslint/parser": "^2.3.2",
        "eslint": "^6.5.1",
        "eslint-config-google": "^0.14.0",
        "microbundle": "^0.11.0",
        "type-fest": "^0.8.1",
        "which-pm": "^2.0.0"
    },
    "resolutions": {
        "microbundle/rollup-plugin-typescript2/rollup-pluginutils/micromatch/braces": "^2.3.1",
        "microbundle/rollup-plugin-postcss/cssnano/postcss-svgo/svgo/js-yaml": "^3.13.0"
    }
}
